		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>
	<test name="Amazon Test">
		<classes>
			<class name="com.simplilearn.testng.Phase2_Selenium_TestNG.Amazon.AmazonHomePageTest"></class>
			<class
				name="com.simplilearn.testng.Phase2_Selenium_TestNG.Amazon.AmazonLinkVerificationCssSelectorTest"></class>
			<class
				name="com.simplilearn.testng.Phase2_Selenium_TestNG.Amazon.AmazonLinkVerificationTest"></class>
			<class
				name="com.simplilearn.testng.Phase2_Selenium_TestNG.Amazon.AmazonLinkVerificationXpathTest"></class>
